{
  "name": "epsilon",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --modern",
    "build-configurator": "npm run build -- --mode electron && mkdirp ../public && cp -r dist/* ../public/",
    "build-license": "npm run build && mkdirp ../public && cp -r dist/* ../public/",
    "build-modern": "vue-cli-service build --modern",
    "clear-jest-cache": "jest --clearCache",
    "http-server": "http-server",
    "lint": "vue-cli-service lint",
    "mkdirp": "mkdirp",
    "dev-proxy": "npm run http-server -- src --p 8090",
    "serve-production": "npm run http-server -- dist --p 4200",
    "test:e2e": "vue-cli-service test:e2e",
    "test:e2e-ci": "npm run test:e2e -- --headless",
    "test:unit": "vue-cli-service test:unit --coverage",
    "vue-service": "vue-cli-service",
    "webpack-visualizer": "npm run build -- --report-json && webpack-bundle-analyzer dist/report.json",
    "gh-pages": "npm run gh-pages-build && npm run gh-pages-deploy",
    "gh-pages-build": "npm run build -- --mode github",
    "gh-pages-deploy": "cd dist && git init && git add -A && git commit -m \"deploy\" && git push -f git@github.com:jojanper/epsilon.git master:gh-pages"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "lodash": "^4.17.15",
    "nprogress": "^0.2.0",
    "rxjs": "^6.5.3",
    "sass": "< 1.33",
    "vee-validate": "^3.4.5",
    "vue": "^2.6.10",
    "vue-i18n": "^8.14.1",
    "vue-router": "^3.1.3",
    "vuetify": "^2.5.10",
    "vuex": "^3.6.0"
  },
  "devDependencies": {
    "@intlify/vue-i18n-loader": "^3.3.0",
    "@vue/cli-plugin-babel": "^4.5.15",
    "@vue/cli-plugin-e2e-cypress": "^4.5.15",
    "@vue/cli-plugin-eslint": "^4.5.15",
    "@vue/cli-plugin-unit-jest": "^4.5.15",
    "@vue/cli-service": "^4.5.15",
    "@vue/eslint-config-airbnb": "^5.3.0",
    "@vue/test-utils": "^1.2.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.3.1",
    "copy-webpack-plugin": "^6.4.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-vue": "^8.0.3",
    "cypress": "^8.7.0",
    "flush-promises": "^1.0.2",
    "http-server": "latest",
    "mkdirp": "^1.0.4",
    "moxios": "^0.4.0",
    "node-sass": "^6.0.1",
    "prettier-eslint": "^13.0.0",
    "sass-loader": "^10",
    "stylus": "^0.55.0",
    "stylus-loader": "^6.2.0",
    "vue-cli-plugin-i18n": "^2.3.1",
    "vue-cli-plugin-vuetify": "^2.4.3",
    "vue-template-compiler": "^2.6.14",
    "webpack-bundle-analyzer": "^4.5.0"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "setupFiles": [
      "<rootDir>/tests/unit/setup"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/tests/unit/setupAfter"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.(js|jsx)?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(vuetify|vee-validate/dist/rules))"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "<rootDir>/(src/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx))"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,vue}",
      "!src/main.js",
      "!src/router.js",
      "!src/store.js"
    ],
    "coverageReporters": [
      "lcov",
      "text",
      "text-summary"
    ]
  },
  "author": "Juha Ojanper√§ <juha.ojanpera@gmail.com>",
  "description": "Vue application playground",
  "engines": {
    "node": ">= 12.0.0",
    "npm": ">= 6.0.0"
  }
}
