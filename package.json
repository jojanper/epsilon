{
  "name": "epsilon",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "http-server": "http-server",
    "lint": "vue-cli-service lint",
    "dev-proxy": "npm run http-server -- src --p 8090",
    "serve-production": "npm run http-server -- dist --p 4200",
    "test:e2e": "vue-cli-service test:e2e",
    "test:e2e-ci": "npm run test:e2e -- --headless",
    "test:unit": "vue-cli-service test:unit --coverage",
    "vue-service": "vue-cli-service",
    "webpack-visualizer": "npm run build -- --report-json && webpack-bundle-analyzer dist/report.json",
    "gh-pages": "npm run gh-pages-build && npm run gh-pages-deploy",
    "gh-pages-build": "npm run build -- --mode github",
    "gh-pages-deploy": "cd dist && git init && git add -A && git commit -m \"deploy\" && git push -f git@github.com:jojanper/epsilon.git master:gh-pages"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "fibers": "^4.0.1",
    "lodash": "^4.17.15",
    "nprogress": "^0.2.0",
    "rxjs": "^6.5.3",
    "sass": "^1.23.0",
    "vue": "^2.6.10",
    "vue-i18n": "^8.14.1",
    "vue-router": "^3.1.3",
    "vuetify": "^2.1.1",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@kazupon/vue-i18n-loader": "^0.3.0",
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-e2e-cypress": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-plugin-unit-jest": "^3.8.0",
    "@vue/cli-service": "^3.8.4",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "copy-webpack-plugin": "^5.0.3",
    "http-server": "latest",
    "moxios": "0.4.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-cli-plugin-i18n": "^0.6.0",
    "vue-cli-plugin-vuetify": "^0.6.3",
    "vue-template-compiler": "^2.6.10",
    "webpack-bundle-analyzer": "^3.3.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "jasmine": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/airbnb"
    ],
    "rules": {
      "comma-dangle": 0,
      "indent": [
        "error",
        4
      ],
      "vue/valid-v-on": 0,
      "vue/no-parsing-error": 0,
      "import/prefer-default-export": 0,
      "no-underscore-dangle": [
        "error",
        {
          "allowAfterThis": true
        }
      ],
      "no-param-reassign": [
        "error",
        {
          "props": true,
          "ignorePropertyModificationsFor": [
            "state"
          ]
        }
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "globals": {
      "createModuleStore": false
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "setupFiles": [
      "<rootDir>/tests/unit/setup"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.(js|jsx)?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(vuetify)/)"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "<rootDir>/(src/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx))"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,vue}",
      "!src/main.js",
      "!src/router.js",
      "!src/store.js"
    ],
    "coverageReporters": [
      "lcov",
      "text",
      "text-summary"
    ]
  },
  "author": "Juha Ojanper√§ <juha.ojanpera@gmail.com>",
  "description": "Vue application playground",
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 5.0.0"
  }
}
